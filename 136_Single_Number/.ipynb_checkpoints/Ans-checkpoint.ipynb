   "outputs": [],
   "source": [
    "class Solution:\n",
    "    def singleNumber(self, nums: list) -> int:\n",
    "        ans = nums[0]\n",
    "        for i in nums[1:]:\n",
    "            ans ^= i\n",
    "        return ans"
   ]
  },
